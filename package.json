{
  "name": "web-namespaces",
  "version": "1.0.0",
  "description": "Map of web namespaces",
  "license": "MIT",
  "keywords": [
    "web",
    "platform",
    "namespace",
    "namespaces",
    "whatwg",
    "w3c"
  ],
  "repository": "https://github.com/wooorm/web-namespaces",
  "bugs": "https://github.com/wooorm/web-namespaces/issues",
  "author": "Titus Wormer <tituswormer@gmail.com> (http://wooorm.com)",
  "contributors": [
    "Titus Wormer <tituswormer@gmail.com> (http://wooorm.com)"
  ],
  "files": [
    "index.js",
    "index.json"
  ],
  "dependencies": {},
  "devDependencies": {
    "bail": "^1.0.0",
    "browserify": "^13.0.0",
    "cheerio": "^0.20.0",
    "esmangle": "^1.0.1",
    "nyc": "^9.0.1",
    "remark-cli": "^2.0.0",
    "remark-preset-wooorm": "^1.0.0",
    "tape": "^4.4.0",
    "xo": "^0.17.1"
  },
  "scripts": {
    "build-md": "remark . --quiet --frail --output",
    "build-crawl": "node script/build.js",
    "build-bundle": "browserify index.js --bare -s webNamespaces > web-namespaces.js",
    "build-mangle": "esmangle web-namespaces.js > web-namespaces.min.js",
    "build": "npm run build-md && npm run build-bundle && npm run build-mangle",
    "lint": "xo",
    "test-api": "node test",
    "test-coverage": "nyc --reporter lcov tape test.js",
    "test": "npm run build && npm run lint && npm run test-coverage"
  },
  "nyc": {
    "check-coverage": true,
    "lines": 100,
    "functions": 100,
    "branches": 100
  },
  "xo": {
    "space": true,
    "ignore": [
      "web-namespaces.js"
    ]
  },
  "remarkConfig": {
    "presets": "wooorm"
  }
}
